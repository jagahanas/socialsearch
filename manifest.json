{
  "manifest_version": 2,
  "name": "Social Search",
//  "default_locale": "en",
  "description": "Search for people on the major social networks: Facebook, Twitter, LinkedIN, Google+, etc",
  "version": "1.0",
  "background":{"scripts": ["background.js"]},

  "browser_action": {
    "default_icon": "socialMedia.png",
//	"default_popup": "popup.html",
    "default_title": "Social Search"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
	"tabs"
  ]
}
